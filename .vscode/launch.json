  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug",
      "cargo": {
        "args": [
          "build",
          "--bin=adventofcode",
          "--package=adventofcode"
        ],
        "filter": {
          "name": "adventofcode",
          "kind": "bin"
        }
      },
      "args": ["--year", "2019", "--day", "9", "--index", "0", "--verbose"],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole"
    }
  ]
}
